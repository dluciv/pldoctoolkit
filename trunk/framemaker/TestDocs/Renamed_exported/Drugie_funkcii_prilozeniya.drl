               <para>
                  <command moreinfo="none">Состояния протокола</command>
                  <xref linkend="MTP" role="Undefined Cross-Reference"/>
                  <command moreinfo="none">, Level 2 (см.
Q.703 Белой Книги)</command>
               </para>
               <informaltable frame="all" colsep="1" rowsep="1">
                  <tgroup cols="2" tgroupstyle="Format A" colsep="1" rowsep="1" align="left">
                     <colspec colnum="1" colname="1" colwidth="1.000in"/>
