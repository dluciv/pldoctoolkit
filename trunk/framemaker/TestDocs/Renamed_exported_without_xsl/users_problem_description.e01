<colspec colnum = "3" colname = "3" colwidth = "0.984in"/>
<thead>
<row rowsep = "1">
<entry colname = "1">Операция</entry>
<entry colname = "2">Пользователи группы RMO Administrators</entry>
<entry colname = "3">Пользователи группы RMO Users</entry>
</row>
</thead>
<tbody>
