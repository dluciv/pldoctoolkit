<Entry id = "ТЭЗ"><Attr id = "abbr">ТЭЗ</Attr><Attr id = "decod">Типовой
элемент замены</Attr></Entry>
<Entry id = "ФПО"><Attr id = "abbr">ФПО</Attr><Attr id = "decod">Функциональное
