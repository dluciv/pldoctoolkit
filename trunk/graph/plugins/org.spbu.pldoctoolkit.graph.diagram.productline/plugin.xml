<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<plugin>

   <extension point="org.eclipse.core.runtime.preferences">
      <initializer class="org.spbu.pldoctoolkit.graph.diagram.productline.part.DrlModelDiagramPreferenceInitializer"/>
   </extension>

  <extension point="org.eclipse.team.core.fileTypes">
      <fileTypes
         type="text"
         extension="productline_diagram">
      </fileTypes>
  </extension>

   <extension point="org.eclipse.emf.ecore.extension_parser">
      <parser
         type="productline_diagram"
         class="org.eclipse.gmf.runtime.emf.core.resources.GMFResourceFactory">
      </parser>
   </extension>

   <extension point="org.eclipse.ui.editors">
     <editor
        id="org.spbu.pldoctoolkit.graph.diagram.productline.part.DrlModelDiagramEditorID"
        name="DrlModel Diagram Editor"
        icon="icons/obj16/DrlDiagramFile.gif"
        extensions="productline_diagram"
        default="true"
        class="org.spbu.pldoctoolkit.graph.diagram.productline.part.DrlModelDiagramEditor"
        matchingStrategy="org.spbu.pldoctoolkit.graph.diagram.productline.part.DrlModelMatchingStrategy"
        contributorClass="org.spbu.pldoctoolkit.graph.diagram.productline.part.DrlModelDiagramActionBarContributor">
     </editor>
   </extension>

   <extension point="org.eclipse.ui.newWizards">
  	  <wizard
  	     name="DrlModel Diagram"
  	     icon="icons/obj16/DrlDiagramFile.gif"
  	     category="org.spbu.pldoctoolkit.graph.diagram.productline.part.DrlModelCreationWizardID"
  	     class="org.spbu.pldoctoolkit.graph.diagram.productline.part.DrlModelCreationWizard"
  	     id="org.spbu.pldoctoolkit.graph.diagram.productline.part.DrlModelCreationWizardID">
  	  	 <description>
  	  		Creates DrlModel diagram.
  	  	 </description>  
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.popupMenus">
      <objectContribution
            id="org.spbu.pldoctoolkit.graph.diagram.productline.ui.objectContribution.IFile1"
            nameFilter="*.drl"
            objectClass="org.eclipse.core.resources.IFile">
         <action
               label="Initialize productline_diagram diagram file"
               class="org.spbu.pldoctoolkit.graph.diagram.productline.part.DrlModelInitDiagramFileAction"
               menubarPath="additions"
               enablesFor="1"
               id="org.spbu.pldoctoolkit.graph.diagram.productline.part.DrlModelInitDiagramFileActionID">
         </action>
      </objectContribution>  
      <objectContribution
            adaptable="false"
            id="org.spbu.pldoctoolkit.graph.diagram.productline.ui.objectContribution.ProductLineEditPart1"
            objectClass="org.spbu.pldoctoolkit.graph.diagram.productline.edit.parts.ProductLineEditPart">
         <action
               class="org.spbu.pldoctoolkit.graph.diagram.productline.part.DrlModelCreateShortcutAction"
               enablesFor="1"
               id="org.spbu.pldoctoolkit.graph.diagram.productline.part.DrlModelCreateShortcutActionID"
               label="Create Shortcut..."
               menubarPath="additions">
         </action>
      </objectContribution>                      
      <objectContribution
            adaptable="false"
            id="org.spbu.pldoctoolkit.graph.diagram.productline.ui.objectContribution.ProductLineEditPart2"
            objectClass="org.spbu.pldoctoolkit.graph.diagram.productline.edit.parts.ProductLineEditPart">
         <action
               class="org.spbu.pldoctoolkit.graph.diagram.productline.part.DrlModelLoadResourceAction"
               enablesFor="1"
               id="org.spbu.pldoctoolkit.graph.diagram.productline.part.DrlModelLoadResourceActionID"
               label="Load Resource..."
               menubarPath="additions">
         </action>
      </objectContribution>                      
  </extension>

   <extension point="org.eclipse.gmf.runtime.common.ui.services.action.contributionItemProviders">
      <contributionItemProvider class="org.spbu.pldoctoolkit.graph.diagram.productline.providers.DrlModelContributionItemProvider">
         <Priority name="Low">
         </Priority>
         <partContribution id="org.spbu.pldoctoolkit.graph.diagram.productline.part.DrlModelDiagramEditorID">
            <partAction menubarPath="/file/print" id="printPreviewAction">
            </partAction>
         </partContribution>
      </contributionItemProvider>
      <contributionItemProvider
            class="org.eclipse.gmf.runtime.diagram.ui.providers.DiagramContributionItemProvider"
            checkPluginLoaded="false">
         <Priority name="Low"/>
         <popupContribution class="org.eclipse.gmf.runtime.diagram.ui.providers.DiagramContextMenuProvider">
            <popupStructuredContributionCriteria objectClass="org.spbu.pldoctoolkit.graph.diagram.productline.edit.parts.PLSchemeEditPart"/>
            <popupPredefinedItem id="deleteFromDiagramAction" remove="true"/>
         </popupContribution>
         <popupContribution class="org.eclipse.gmf.runtime.diagram.ui.providers.DiagramContextMenuProvider">
            <popupStructuredContributionCriteria objectClass="org.spbu.pldoctoolkit.graph.diagram.productline.edit.parts.ProductEditPart"/>
            <popupPredefinedItem id="deleteFromDiagramAction" remove="true"/>
         </popupContribution>
         <popupContribution class="org.eclipse.gmf.runtime.diagram.ui.providers.DiagramContextMenuProvider">
            <popupStructuredContributionCriteria objectClass="org.spbu.pldoctoolkit.graph.diagram.productline.edit.parts.ProductNameEditPart"/>
            <popupPredefinedItem id="deleteFromDiagramAction" remove="true"/>
         </popupContribution>
         <popupContribution class="org.eclipse.gmf.runtime.diagram.ui.providers.DiagramContextMenuProvider">
            <popupStructuredContributionCriteria objectClass="org.spbu.pldoctoolkit.graph.diagram.productline.edit.parts.ProductLine2EditPart"/>
            <popupPredefinedItem id="deleteFromDiagramAction" remove="true"/>
         </popupContribution>
         <popupContribution class="org.eclipse.gmf.runtime.diagram.ui.providers.DiagramContextMenuProvider">
            <popupStructuredContributionCriteria objectClass="org.spbu.pldoctoolkit.graph.diagram.productline.edit.parts.ProductLineNameEditPart"/>
            <popupPredefinedItem id="deleteFromDiagramAction" remove="true"/>
         </popupContribution>
      </contributionItemProvider>
   </extension>

   <extension point="org.eclipse.gmf.runtime.common.ui.services.action.globalActionHandlerProviders">
      <GlobalActionHandlerProvider
         class="org.eclipse.gmf.runtime.diagram.ui.providers.DiagramGlobalActionHandlerProvider"
         id="DrlModelPresentation">
         <Priority name="Lowest"/>
         <ViewId id="org.spbu.pldoctoolkit.graph.diagram.productline.part.DrlModelDiagramEditorID">
            <ElementType class="org.eclipse.gmf.runtime.diagram.ui.editparts.IGraphicalEditPart">
               <GlobalActionId actionId="delete"/>
            </ElementType>
            <ElementType class="org.eclipse.gmf.runtime.diagram.ui.editparts.DiagramEditPart">
               <GlobalActionId actionId="save"/>
            </ElementType>
         </ViewId>
      </GlobalActionHandlerProvider>
      <GlobalActionHandlerProvider
         class="org.eclipse.gmf.runtime.diagram.ui.printing.render.providers.DiagramWithPrintGlobalActionHandlerProvider"
         id="DrlModelPresentationPrint">
         <Priority name="Lowest"/>
         <ViewId id="org.spbu.pldoctoolkit.graph.diagram.productline.part.DrlModelDiagramEditorID">
            <ElementType class="org.eclipse.gmf.runtime.diagram.ui.editparts.IGraphicalEditPart">
               <GlobalActionId actionId="print"/>
            </ElementType>
         </ViewId>
      </GlobalActionHandlerProvider>
      <GlobalActionHandlerProvider
         class="org.eclipse.gmf.runtime.diagram.ui.providers.ide.providers.DiagramIDEGlobalActionHandlerProvider"
         id="DrlModelPresentationIDE">
         <Priority name="Lowest"/>
         <ViewId id="org.spbu.pldoctoolkit.graph.diagram.productline.part.DrlModelDiagramEditorID">
            <ElementType class="org.eclipse.gmf.runtime.diagram.ui.editparts.IGraphicalEditPart">
               <GlobalActionId actionId="bookmark"/>
            </ElementType>
         </ViewId>
      </GlobalActionHandlerProvider>
      <GlobalActionHandlerProvider
            class="org.eclipse.gmf.runtime.diagram.ui.render.providers.DiagramUIRenderGlobalActionHandlerProvider"
            id="DrlModelRender">
         <Priority name="Lowest"/>
         <ViewId id="org.spbu.pldoctoolkit.graph.diagram.productline.part.DrlModelDiagramEditorID">
            <ElementType class="org.eclipse.gmf.runtime.diagram.ui.editparts.IGraphicalEditPart">
               <GlobalActionId actionId="cut"/>
               <GlobalActionId actionId="copy"/>
               <GlobalActionId actionId="paste"/>
            </ElementType>
         </ViewId>
      </GlobalActionHandlerProvider>
   </extension>

   <extension point="org.eclipse.gmf.runtime.diagram.core.viewProviders">
      <viewProvider class="org.spbu.pldoctoolkit.graph.diagram.productline.providers.DrlModelViewProvider">
         <Priority name="Lowest"/>
         <context viewClass="org.eclipse.gmf.runtime.notation.Diagram" semanticHints="DrlModel"/>
         <context viewClass="org.eclipse.gmf.runtime.notation.Node" semanticHints=""/>
         <context viewClass="org.eclipse.gmf.runtime.notation.Edge" semanticHints=""/>
      </viewProvider>
   </extension>

   <extension point="org.eclipse.gmf.runtime.diagram.ui.editpartProviders">
      <editpartProvider class="org.spbu.pldoctoolkit.graph.diagram.productline.providers.DrlModelEditPartProvider">
         <Priority name="Lowest"/>
      </editpartProvider>
   </extension>

   <extension point="org.eclipse.gmf.runtime.diagram.ui.paletteProviders">
      <paletteProvider class="org.spbu.pldoctoolkit.graph.diagram.productline.providers.DrlModelPaletteProvider">
         <Priority name="Lowest"/>
         <editor id="org.spbu.pldoctoolkit.graph.diagram.productline.part.DrlModelDiagramEditorID"/>
      </paletteProvider>
   </extension>

   <extension point="org.eclipse.gmf.runtime.emf.ui.modelingAssistantProviders">
      <modelingAssistantProvider class="org.spbu.pldoctoolkit.graph.diagram.productline.providers.DrlModelModelingAssistantProvider">
         <Priority name="Lowest"/>
      </modelingAssistantProvider>
   </extension>

   <extension point="org.eclipse.gmf.runtime.common.ui.services.properties.propertiesProviders">
      <PropertiesProvider
            verifyPluginLoaded="false"
            class="org.spbu.pldoctoolkit.graph.diagram.productline.providers.DrlModelPropertyProvider">
         <Priority name="Lowest"/>
      </PropertiesProvider>
   </extension>

   <extension point="org.eclipse.gmf.runtime.common.ui.services.iconProviders">
      <IconProvider class="org.spbu.pldoctoolkit.graph.diagram.productline.providers.DrlModelIconProvider">
         <Priority name="Low"/>
      </IconProvider>
   </extension>

   <extension point="org.eclipse.gmf.runtime.common.ui.services.parserProviders">
      <ParserProvider class="org.spbu.pldoctoolkit.graph.diagram.productline.providers.DrlModelParserProvider">
         <Priority name="Lowest"/>
      </ParserProvider>
   </extension>

   <extension point="org.eclipse.gmf.runtime.diagram.ui.decoratorProviders">
      <decoratorProvider class="org.spbu.pldoctoolkit.graph.diagram.productline.providers.DrlModelShortcutsDecoratorProvider">
         <Priority name="Lowest"/>
      </decoratorProvider>
   </extension>

   <extension point="org.eclipse.gmf.runtime.emf.type.core.elementTypes">

      <metamodel nsURI="http://tepkom.ru/drl">
         <metamodelType
               id="org.spbu.pldoctoolkit.graph.diagram.productline.ProductLine_79"
               kind="org.eclipse.gmf.runtime.emf.type.core.IHintedType"
               eclass="ProductLine"
               edithelper="org.spbu.pldoctoolkit.graph.diagram.productline.edit.helpers.ProductLineEditHelper">
            <param name="semanticHint" value="79"/>
         </metamodelType>
      </metamodel>

      <metamodel nsURI="http://tepkom.ru/drl">
         <metamodelType
               id="org.spbu.pldoctoolkit.graph.diagram.productline.PLScheme_2001"
               kind="org.eclipse.gmf.runtime.emf.type.core.IHintedType"
               eclass="PLScheme"
               edithelper="org.spbu.pldoctoolkit.graph.diagram.productline.edit.helpers.PLSchemeEditHelper">
            <param name="semanticHint" value="2001"/>
         </metamodelType>
      </metamodel>

      <metamodel nsURI="http://tepkom.ru/drl">
         <metamodelType
               id="org.spbu.pldoctoolkit.graph.diagram.productline.Product_2002"
               name="Product"
               kind="org.eclipse.gmf.runtime.emf.type.core.IHintedType"
               eclass="Product"
               edithelper="org.spbu.pldoctoolkit.graph.diagram.productline.edit.helpers.ProductEditHelper">
            <param name="semanticHint" value="2002"/>
         </metamodelType>
      </metamodel>

      <metamodel nsURI="http://tepkom.ru/drl">
         <specializationType
               id="org.spbu.pldoctoolkit.graph.diagram.productline.ProductLine_1001"
               kind="org.eclipse.gmf.runtime.emf.type.core.IHintedType"
               edithelperadvice="org.spbu.pldoctoolkit.graph.diagram.productline.edit.helpers.ProductLineEditHelperAdvice">
            <specializes id="org.spbu.pldoctoolkit.graph.diagram.productline.ProductLine_79"/>
            <param name="semanticHint" value="1001"/>
         </specializationType>
      </metamodel>
   </extension>

   <extension point="org.eclipse.gmf.runtime.emf.type.core.elementTypeBindings">
      <clientContext id="DrlModelClientContext">
         <enablement>
            <test
               property="org.eclipse.gmf.runtime.emf.core.editingDomain"
               value="org.spbu.pldoctoolkit.graph.diagram.productline.EditingDomain"/>
         </enablement>
      </clientContext> 
      <binding context="DrlModelClientContext">
         <elementType ref="org.spbu.pldoctoolkit.graph.diagram.productline.ProductLine_79"/>
         <elementType ref="org.spbu.pldoctoolkit.graph.diagram.productline.PLScheme_2001"/>
         <elementType ref="org.spbu.pldoctoolkit.graph.diagram.productline.Product_2002"/>
         <elementType ref="org.spbu.pldoctoolkit.graph.diagram.productline.ProductLine_1001"/>
         <advice ref="org.eclipse.gmf.runtime.diagram.core.advice.notationDepdendents"/>
      </binding>
   </extension>

</plugin>