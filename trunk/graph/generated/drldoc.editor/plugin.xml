<plugin>


  <extension
    point = "org.eclipse.ui.newWizards">
    <category
      id = "org.eclipse.emf.ecore.Wizard.category.ID"
      name="%_UI_Wizard_category">
    </category>
    <wizard
      id = "org.spbu.pldoctoolkit.drlvisual.presentation.DrlModelWizardID"
      name = "%_UI_drlModelWizard_label"
      class = "org.spbu.pldoctoolkit.drlvisual.presentation.DrlModelWizard"
      category = "org.eclipse.emf.ecore.Wizard.category.ID"
      icon = "icons/full/obj16/DrlModelFile.gif">
      <description>%_UI_drlModelWizard_description</description>
      <selection class = "org.eclipse.core.resources.IResource" />
    </wizard>
  </extension>

  <extension point = "org.eclipse.ui.editors">
    <editor
        id = "org.spbu.pldoctoolkit.drlvisual.presentation.DrlEditorID"
        name = "%_UI_drlEditor_label"
        icon = "icons/full/obj16/DrlModelFile.gif"
        extensions = "drl"
        class = "org.spbu.pldoctoolkit.drlvisual.presentation.DrlEditor"
        contributorClass="org.spbu.pldoctoolkit.drlvisual.presentation.DrlActionBarContributor" >
    </editor>
  </extension>
</plugin>
