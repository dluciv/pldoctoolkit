<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>

<plugin>

   <extension point="org.eclipse.core.runtime.preferences">
      <initializer class="org.spbu.pldoctoolkit.drlvisual.diagram.part.DrlModelDiagramPreferenceInitializer"/>
   </extension>

  <extension point="org.eclipse.team.core.fileTypes">
      <fileTypes
         type="text"
         extension="drl_diagram">
      </fileTypes>
  </extension>

   <extension point="org.eclipse.emf.ecore.extension_parser">
      <parser
         type="drl_diagram"
         class="org.eclipse.gmf.runtime.emf.core.resources.GMFResourceFactory">
      </parser>
   </extension>

   <extension point="org.eclipse.ui.editors">
     <editor
        id="org.spbu.pldoctoolkit.drlvisual.diagram.part.DrlModelDiagramEditorID"
        name="DrlModel Diagram Editor"
        icon="icons/obj16/DrlDiagramFile.gif"
        extensions="drl_diagram"
        default="true"
        class="org.spbu.pldoctoolkit.drlvisual.diagram.part.DrlModelDiagramEditor"
        matchingStrategy="org.spbu.pldoctoolkit.drlvisual.diagram.part.DrlModelMatchingStrategy"
        contributorClass="org.spbu.pldoctoolkit.drlvisual.diagram.part.DrlModelDiagramActionBarContributor">
     </editor>
   </extension>

   <extension point="org.eclipse.ui.newWizards">
  	  <wizard
  	     name="DrlModel Diagram"
  	     icon="icons/obj16/DrlDiagramFile.gif"
  	     category="org.eclipse.ui.Examples"
  	     class="org.spbu.pldoctoolkit.drlvisual.diagram.part.DrlModelCreationWizard"
  	     id="org.spbu.pldoctoolkit.drlvisual.diagram.part.DrlModelCreationWizardID">
  	  	 <description>
  	  		Creates DrlModel diagram.
  	  	 </description>  
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.popupMenus">
      <objectContribution
            id="drldoc.diagram.ui.objectContribution.IFile1"
            nameFilter="*.drl"
            objectClass="org.eclipse.core.resources.IFile">
         <action
               label="Initialize drl_diagram diagram file"
               class="org.spbu.pldoctoolkit.drlvisual.diagram.part.DrlModelInitDiagramFileAction"
               menubarPath="additions"
               enablesFor="1"
               id="org.spbu.pldoctoolkit.drlvisual.diagram.part.DrlModelInitDiagramFileActionID">
         </action>
      </objectContribution>  
      <objectContribution
            adaptable="false"
            id="drldoc.diagram.ui.objectContribution.DocumentationCoreEditPart2"
            objectClass="org.spbu.pldoctoolkit.drlvisual.diagram.edit.parts.DocumentationCoreEditPart">
         <action
               class="org.spbu.pldoctoolkit.drlvisual.diagram.part.DrlModelLoadResourceAction"
               enablesFor="1"
               id="org.spbu.pldoctoolkit.drlvisual.diagram.part.DrlModelLoadResourceActionID"
               label="Load Resource..."
               menubarPath="additions">
         </action>
      </objectContribution>                      
  </extension>

   <extension point="org.eclipse.gmf.runtime.common.ui.services.action.contributionItemProviders">
      <contributionItemProvider
            class="org.eclipse.gmf.runtime.diagram.ui.providers.DiagramContributionItemProvider"
            checkPluginLoaded="false">
         <Priority name="Low"/>
         <popupContribution class="org.eclipse.gmf.runtime.diagram.ui.providers.DiagramContextMenuProvider">
            <popupStructuredContributionCriteria objectClass="org.spbu.pldoctoolkit.drlvisual.diagram.edit.parts.InfElementEditPart"/>
            <popupPredefinedItem id="deleteFromDiagramAction" remove="true"/>
         </popupContribution>
         <popupContribution class="org.eclipse.gmf.runtime.diagram.ui.providers.DiagramContextMenuProvider">
            <popupStructuredContributionCriteria objectClass="org.spbu.pldoctoolkit.drlvisual.diagram.edit.parts.InfElementNameIdEditPart"/>
            <popupPredefinedItem id="deleteFromDiagramAction" remove="true"/>
         </popupContribution>
         <popupContribution class="org.eclipse.gmf.runtime.diagram.ui.providers.DiagramContextMenuProvider">
            <popupStructuredContributionCriteria objectClass="org.spbu.pldoctoolkit.drlvisual.diagram.edit.parts.InfProductEditPart"/>
            <popupPredefinedItem id="deleteFromDiagramAction" remove="true"/>
         </popupContribution>
         <popupContribution class="org.eclipse.gmf.runtime.diagram.ui.providers.DiagramContextMenuProvider">
            <popupStructuredContributionCriteria objectClass="org.spbu.pldoctoolkit.drlvisual.diagram.edit.parts.InfProductNameIdEditPart"/>
            <popupPredefinedItem id="deleteFromDiagramAction" remove="true"/>
         </popupContribution>
         <popupContribution class="org.eclipse.gmf.runtime.diagram.ui.providers.DiagramContextMenuProvider">
            <popupStructuredContributionCriteria objectClass="org.spbu.pldoctoolkit.drlvisual.diagram.edit.parts.InfElemRefGroupEditPart"/>
            <popupPredefinedItem id="deleteFromDiagramAction" remove="true"/>
         </popupContribution>
         <popupContribution class="org.eclipse.gmf.runtime.diagram.ui.providers.DiagramContextMenuProvider">
            <popupStructuredContributionCriteria objectClass="org.spbu.pldoctoolkit.drlvisual.diagram.edit.parts.InfElemRefGroupNameEditPart"/>
            <popupPredefinedItem id="deleteFromDiagramAction" remove="true"/>
         </popupContribution>
         <popupContribution class="org.eclipse.gmf.runtime.diagram.ui.providers.DiagramContextMenuProvider">
            <popupStructuredContributionCriteria objectClass="org.spbu.pldoctoolkit.drlvisual.diagram.edit.parts.InfElemRefEditPart"/>
            <popupPredefinedItem id="deleteFromDiagramAction" remove="true"/>
         </popupContribution>
         <popupContribution class="org.eclipse.gmf.runtime.diagram.ui.providers.DiagramContextMenuProvider">
            <popupStructuredContributionCriteria objectClass="org.spbu.pldoctoolkit.drlvisual.diagram.edit.parts.InfElemRefIdEditPart"/>
            <popupPredefinedItem id="deleteFromDiagramAction" remove="true"/>
            <popupPredefinedItem id="deleteFromModelAction" remove="true"/>
         </popupContribution>
         <popupContribution class="org.eclipse.gmf.runtime.diagram.ui.providers.DiagramContextMenuProvider">
            <popupStructuredContributionCriteria objectClass="org.spbu.pldoctoolkit.drlvisual.diagram.edit.parts.SubelementedElementElementsEditPart"/>
            <popupAction path="/editGroup" id="deleteFromModelAction"/>
            <popupPredefinedItem id="deleteFromDiagramAction" remove="true"/>
         </popupContribution>
      </contributionItemProvider>
   </extension>

   <extension point="org.eclipse.gmf.runtime.common.ui.services.action.globalActionHandlerProviders">
      <GlobalActionHandlerProvider
         class="org.eclipse.gmf.runtime.diagram.ui.providers.DiagramGlobalActionHandlerProvider"
         id="DrlModelPresentation">
         <Priority name="Lowest"/>
         <ViewId id="org.spbu.pldoctoolkit.drlvisual.diagram.part.DrlModelDiagramEditorID">
            <ElementType class="org.eclipse.gmf.runtime.diagram.ui.editparts.IGraphicalEditPart">
               <GlobalActionId actionId="delete"/>
            </ElementType>
            <ElementType class="org.eclipse.gmf.runtime.diagram.ui.editparts.DiagramEditPart">
               <GlobalActionId actionId="save"/>
            </ElementType>
         </ViewId>
      </GlobalActionHandlerProvider>
      <GlobalActionHandlerProvider
         class="org.eclipse.gmf.runtime.diagram.ui.providers.ide.providers.DiagramIDEGlobalActionHandlerProvider"
         id="DrlModelPresentationIDE">
         <Priority name="Lowest"/>
         <ViewId id="org.spbu.pldoctoolkit.drlvisual.diagram.part.DrlModelDiagramEditorID">
            <ElementType class="org.eclipse.gmf.runtime.diagram.ui.editparts.IGraphicalEditPart">
               <GlobalActionId actionId="bookmark"/>
            </ElementType>
         </ViewId>
      </GlobalActionHandlerProvider>
      <GlobalActionHandlerProvider
            class="org.eclipse.gmf.runtime.diagram.ui.render.providers.DiagramUIRenderGlobalActionHandlerProvider"
            id="DrlModelRender">
         <Priority name="Lowest"/>
         <ViewId id="org.spbu.pldoctoolkit.drlvisual.diagram.part.DrlModelDiagramEditorID">
            <ElementType class="org.eclipse.gmf.runtime.diagram.ui.editparts.IGraphicalEditPart">
               <GlobalActionId actionId="cut"/>
               <GlobalActionId actionId="copy"/>
               <GlobalActionId actionId="paste"/>
            </ElementType>
         </ViewId>
      </GlobalActionHandlerProvider>
   </extension>

   <extension point="org.eclipse.gmf.runtime.diagram.core.viewProviders">
      <viewProvider class="org.spbu.pldoctoolkit.drlvisual.diagram.providers.DrlModelViewProvider">
         <Priority name="Lowest"/>
         <context viewClass="org.eclipse.gmf.runtime.notation.Diagram" semanticHints="DrlModel"/>
         <context viewClass="org.eclipse.gmf.runtime.notation.Node" semanticHints=""/>
         <context viewClass="org.eclipse.gmf.runtime.notation.Edge" semanticHints=""/>
      </viewProvider>
   </extension>

   <extension point="org.eclipse.gmf.runtime.diagram.ui.editpartProviders">
      <editpartProvider class="org.spbu.pldoctoolkit.drlvisual.diagram.providers.DrlModelEditPartProvider">
         <Priority name="Lowest"/>
      </editpartProvider>
   </extension>

   <extension point="org.eclipse.gmf.runtime.diagram.ui.paletteProviders">
      <paletteProvider class="org.spbu.pldoctoolkit.drlvisual.diagram.providers.DrlModelPaletteProvider">
         <Priority name="Lowest"/>
         <editor id="org.spbu.pldoctoolkit.drlvisual.diagram.part.DrlModelDiagramEditorID"/>
      </paletteProvider>
   </extension>

   <extension point="org.eclipse.gmf.runtime.emf.ui.modelingAssistantProviders">
      <modelingAssistantProvider class="org.spbu.pldoctoolkit.drlvisual.diagram.providers.DrlModelModelingAssistantProvider">
         <Priority name="Lowest"/>
      </modelingAssistantProvider>
   </extension>

   <extension point="org.eclipse.gmf.runtime.common.ui.services.properties.propertiesProviders">
      <PropertiesProvider
            verifyPluginLoaded="false"
            class="org.spbu.pldoctoolkit.drlvisual.diagram.providers.DrlModelPropertyProvider">
         <Priority name="Lowest"/>
      </PropertiesProvider>
   </extension>

   <extension point="org.eclipse.gmf.runtime.common.ui.services.iconProviders">
      <IconProvider class="org.spbu.pldoctoolkit.drlvisual.diagram.providers.DrlModelIconProvider">
         <Priority name="Low"/>
      </IconProvider>
   </extension>

   <extension point="org.eclipse.gmf.runtime.common.ui.services.parserProviders">
      <ParserProvider class="org.spbu.pldoctoolkit.drlvisual.diagram.providers.DrlModelParserProvider">
         <Priority name="Lowest"/>
      </ParserProvider>
   </extension>

   <extension point="org.eclipse.gmf.runtime.emf.type.core.elementTypes">

      <metamodel nsURI="http://tepkom.ru/drl">
         <metamodelType
               id="drldoc.diagram.DocumentationCore_79"
               kind="org.eclipse.gmf.runtime.emf.type.core.IHintedType"
               eclass="DocumentationCore"
               edithelper="org.spbu.pldoctoolkit.drlvisual.diagram.edit.helpers.DocumentationCoreEditHelper">
            <param name="semanticHint" value="79"/>
         </metamodelType>
      </metamodel>

      <metamodel nsURI="http://tepkom.ru/drl">
         <metamodelType
               id="drldoc.diagram.InfElement_1001"
               name="InfElement"
               kind="org.eclipse.gmf.runtime.emf.type.core.IHintedType"
               eclass="InfElement"
               edithelper="org.spbu.pldoctoolkit.drlvisual.diagram.edit.helpers.InfElementEditHelper">
            <param name="semanticHint" value="1001"/>
         </metamodelType>
      </metamodel>

      <metamodel nsURI="http://tepkom.ru/drl">
         <metamodelType
               id="drldoc.diagram.InfProduct_1002"
               name="InfProduct"
               kind="org.eclipse.gmf.runtime.emf.type.core.IHintedType"
               eclass="InfProduct"
               edithelper="org.spbu.pldoctoolkit.drlvisual.diagram.edit.helpers.InfProductEditHelper">
            <param name="semanticHint" value="1002"/>
         </metamodelType>
      </metamodel>

      <metamodel nsURI="http://tepkom.ru/drl">
         <metamodelType
               id="drldoc.diagram.InfElemRefGroup_1003"
               name="SubelementedElementElements"
               kind="org.eclipse.gmf.runtime.emf.type.core.IHintedType"
               eclass="InfElemRefGroup"
               edithelper="org.spbu.pldoctoolkit.drlvisual.diagram.edit.helpers.InfElemRefGroupEditHelper">
            <param name="semanticHint" value="1003"/>
         </metamodelType>
      </metamodel>

      <metamodel nsURI="http://tepkom.ru/drl">
         <metamodelType
               id="drldoc.diagram.InfElemRef_3001"
               name="InfElemRef"
               kind="org.eclipse.gmf.runtime.emf.type.core.IHintedType"
               eclass="InfElemRef"
               edithelper="org.spbu.pldoctoolkit.drlvisual.diagram.edit.helpers.InfElemRefEditHelper">
            <param name="semanticHint" value="3001"/>
         </metamodelType>
      </metamodel>

      <metamodel nsURI="http://tepkom.ru/drl">
         <specializationType
               id="drldoc.diagram.SubelementedElementElements_3002"
               kind="org.eclipse.gmf.runtime.emf.type.core.IHintedType"
               edithelperadvice="org.spbu.pldoctoolkit.drlvisual.diagram.edit.helpers.SubelementedElementElementsEditHelperAdvice">
            <specializes id="org.eclipse.gmf.runtime.emf.type.core.null"/>
            <param name="semanticHint" value="3002"/>
         </specializationType>
      </metamodel>
   </extension>

   <extension point="org.eclipse.gmf.runtime.emf.type.core.elementTypeBindings">
      <clientContext id="DrlModelClientContext">
         <enablement>
            <test
               property="org.eclipse.gmf.runtime.emf.core.editingDomain"
               value="drldoc.diagram.EditingDomain"/>
         </enablement>
      </clientContext> 
      <binding context="DrlModelClientContext">
         <elementType ref="drldoc.diagram.DocumentationCore_79"/>
         <elementType ref="drldoc.diagram.InfElement_1001"/>
         <elementType ref="drldoc.diagram.InfProduct_1002"/>
         <elementType ref="drldoc.diagram.InfElemRefGroup_1003"/>
         <elementType ref="drldoc.diagram.InfElemRef_3001"/>
         <elementType ref="drldoc.diagram.SubelementedElementElements_3002"/>
         <advice ref="org.eclipse.gmf.runtime.diagram.core.advice.notationDepdendents"/>
      </binding>
   </extension>

</plugin>