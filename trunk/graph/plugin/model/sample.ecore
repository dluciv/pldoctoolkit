<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="vardoc"
    nsURI="http://tepkom.ru/drl" nsPrefix="v">
  <eClassifiers xsi:type="ecore:EEnum" name="NestPointType">
    <eLiterals name="INSERT_BEFORE" value="1"/>
    <eLiterals name="INSERT_AFTER" value="2"/>
    <eLiterals name="REPLACE" value="3"/>
    <eLiterals name="REMOVE" value="4"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Element" abstract="true"/>
  <eClassifiers xsi:type="ecore:EClass" name="DocumentPart" eSuperTypes="#//ReusablePart"/>
  <eClassifiers xsi:type="ecore:EClass" name="Family" eSuperTypes="#//ReusablePart"/>
  <eClassifiers xsi:type="ecore:EClass" name="Document" eSuperTypes="#//Element">
    <eStructuralFeatures xsi:type="ecore:EReference" name="adapters" upperBound="-1"
        eType="#//Adapter" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="family" lowerBound="1"
        eType="#//Family"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="variables" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EMap"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="partRefs" upperBound="-1"
        eType="#//DocumentPartRef"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Adapter">
    <eStructuralFeatures xsi:type="ecore:EReference" name="nestPointRefs" upperBound="-1"
        eType="#//NestPointRef" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="documentPartRef" lowerBound="1"
        eType="#//DocumentPartRef"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="NestPoint" eSuperTypes="#//InnerElement">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="id" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"
        iD="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="InnerElement" abstract="true" interface="true"
      eSuperTypes="#//Element">
    <eStructuralFeatures xsi:type="ecore:EReference" name="connection" eType="#//Connection"
        eOpposite="#//Connection/target"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Condition" eSuperTypes="#//InnerElement #//SubelementedElement">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="condition" lowerBound="1"
        eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="NestPointRef">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="type" lowerBound="1" eType="#//NestPointType"
        defaultValueLiteral=""/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="text" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="nestPoint" lowerBound="1"
        eType="#//NestPoint"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ReusablePart" abstract="true" eSuperTypes="#//Element #//SubelementedElement">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="id" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"
        derived="true" iD="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DocumentPartRef" eSuperTypes="#//InnerElement">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="id" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"
        iD="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="part" lowerBound="1" eType="#//DocumentPart"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="connection" eType="#//GroupConnection"
        eOpposite="#//GroupConnection/target"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="DocumentPartRefGroup" eSuperTypes="#//InnerElement">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="id" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"
        iD="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="modifier" lowerBound="1"
        eType="#//GroupType"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="documentPartRefs" upperBound="-1"
        eType="#//GroupConnection" containment="true" eOpposite="#//GroupConnection/source"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="GroupType">
    <eLiterals name="OR" value="1"/>
    <eLiterals name="XOR" value="2"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Schema">
    <eStructuralFeatures xsi:type="ecore:EReference" name="part" eType="#//ReusablePart"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Connection">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="type" lowerBound="1" eType="#//ConnectionType"
        defaultValueLiteral=""/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="source" lowerBound="1"
        eType="#//SubelementedElement" eOpposite="#//SubelementedElement/elements"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="target" lowerBound="1"
        eType="#//InnerElement" eOpposite="#//InnerElement/connection"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EEnum" name="ConnectionType">
    <eLiterals name="MANDATORY"/>
    <eLiterals name="OPTIONAL" value="1"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SubelementedElement">
    <eStructuralFeatures xsi:type="ecore:EReference" name="elements" upperBound="-1"
        eType="#//Connection" containment="true" eOpposite="#//Connection/source"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="GroupConnection">
    <eStructuralFeatures xsi:type="ecore:EReference" name="target" lowerBound="1"
        eType="#//DocumentPartRef" eOpposite="#//DocumentPartRef/connection"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="source" lowerBound="1"
        eType="#//DocumentPartRefGroup" eOpposite="#//DocumentPartRefGroup/documentPartRefs"/>
  </eClassifiers>
</ecore:EPackage>
